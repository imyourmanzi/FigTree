# File: git/provided.gitconfig
# Author: Matt Manzi
# Date: 2021-02-13

[alias]
    a = add
    ck = checkout
    ci = commit
    cia = commit -a
    st = status
    br = branch
    f = fetch
    fa = fetch --all
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    unstage = reset HEAD --
    pa = pull --all
    pl = pull
    ps = push

[init]
    defaultBranch = main
