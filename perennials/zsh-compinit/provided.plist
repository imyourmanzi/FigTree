<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.figtree.zsh.compinit</string>

        <key>Program</key>
        <string>/bin/zsh</string>

        <key>ProgramArguments</key>
        <array>
            <string>-lic</string>
            <string>"autoload -Uz compinit; compinit"</string>
        </array>

        <key>Nice</key>
        <integer>1</integer>

        <key>StartCalendarInterval</key>
        <dict>
            <key>Minute</key>
            <integer>5</integer>
            <key>Hour</key>
            <integer>4</integer>
        </dict>

        <key>RunAtLoad</key>
        <true/>
    </dict>
</plist>
